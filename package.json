{
  "name": "@egomobile/forms",
  "version": "0.5.0",
  "description": "Types and helpers, handling forms and their data.",
  "main": "lib/index.js",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "CHANGELOG.md",
    "LICENSE",
    "lib",
    "!lib/**/*.map",
    "README.md"
  ],
  "keywords": [
    "node",
    "js",
    "ts",
    "javascript",
    "typescript",
    "form",
    "function",
    "class",
    "helper",
    "type",
    "interface",
    "data",
    "ajv",
    "schema",
    "validator",
    "validation",
    "compiler",
    "compile",
    "ego"
  ],
  "scripts": {
    "build": "del ./lib && tsc",
    "docs:generate": "del ./docs && typedoc --out ./docs ./src",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint:fix": "eslint --fix -c .eslintrc.js --ext .ts src",
    "sandbox": "nodemon --watch './sandbox/index.ts' ./sandbox/index.ts --inspect=0.0.0.0:9229"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/egomobile/node-forms"
  },
  "author": "Next.e.GO Mobile SE, Aachen, Germany",
  "license": "LGPL-3.0",
  "devDependencies": {
    "@egomobile/tsconfig": "^5.0.0",
    "@types/node": "14.18.22",
    "del-cli": "5.0.0",
    "eslint": "8.23.0",
    "eslint-config-ego": "^0.17.1",
    "nodemon": "2.0.19",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "typedoc": "0.23.11",
    "typescript": "4.6.4"
  },
  "dependencies": {
    "@types/json-schema": "7.0.11",
    "ajv": "8.11.0",
    "ajv-formats": "2.1.1",
    "fast-xml-parser": "4.0.9",
    "json5": "2.2.1"
  }
}
